{
  "name": "use-google-recaptcha",
  "version": "1.1.0",
  "description": "A React hook for Google reCAPTCHA v3 integration with TypeScript support.",
  "license": "MIT",
  "author": {
    "name": "Dmitrii Selikhov",
    "email": "dmitrii.selikhov@gmail.com",
    "url": "https://www.linkedin.com/in/dimetrix"
  },
  "contributors": [
    {
      "name": "Dmitrii Selikhov",
      "email": "dmitrii.selikhov@gmail.com",
      "url": "https://www.linkedin.com/in/dimetrix"
    }
  ],
  "email": "dmitrii.selikhov@gmail.com",
  "scripts": {
    "build": "tsup",
    "watch": "tsup ./src --watch",
    "run": "node dist/index.js",
    "dev": "pnpm run watch & nodemon dist/index.js",
    "prettier": "prettier --write src/**",
    "format": "pnpm run prettier && pnpm type",
    "bootstrap": "pnpm install && pnpm format",
    "type": "tsc --noEmit --incremental",
    "tsc:clean": "tsc --build --clean",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "homepage": "https://github.com/idimetrix/use-google-recaptcha#readme",
  "url": "https://github.com/idimetrix/use-google-recaptcha/issue",
  "bugs": {
    "url": "https://github.com/idimetrix/use-google-recaptcha/issues"
  },
  "keywords": [
    "react",
    "recaptcha",
    "google",
    "v3",
    "hook",
    "typescript",
    "captcha",
    "security",
    "spam",
    "protection"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "dependencies": {
    "react": "18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "14.3.1",
    "@types/jest": "29.5.13",
    "@types/node": "22.5.5",
    "@types/react": "19.1.13",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "jest-environment-jsdom": "30.1.2",
    "lint-staged": "15.2.10",
    "nodemon": "3.1.4",
    "prettier": "3.3.3",
    "react-dom": "18.3.1",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsup": "8.5.0",
    "typescript": "5.6.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
